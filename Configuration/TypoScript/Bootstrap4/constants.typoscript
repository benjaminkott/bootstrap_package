plugin.bootstrap_package {
    settings {
        scss {

            # cat=bootstrap 4.x: styling/300/white; type=string; label=$white:
            white = #fff
            # cat=bootstrap 4.x: styling/300/gray-100; type=string; label=$gray-100:
            gray-100 = #f8f9fa
            # cat=bootstrap 4.x: styling/300/gray-200; type=string; label=$gray-200:
            gray-200 = #e9ecef
            # cat=bootstrap 4.x: styling/300/gray-300; type=string; label=$gray-300:
            gray-300 = #dee2e6
            # cat=bootstrap 4.x: styling/300/gray-400; type=string; label=$gray-400:
            gray-400 = #ced4da
            # cat=bootstrap 4.x: styling/300/gray-500; type=string; label=$gray-500:
            gray-500 = #adb5bd
            # cat=bootstrap 4.x: styling/300/gray-600; type=string; label=$gray-600:
            gray-600 = #6c757d
            # cat=bootstrap 4.x: styling/300/gray-700; type=string; label=$gray-700:
            gray-700 = #495057
            # cat=bootstrap 4.x: styling/300/gray-800; type=string; label=$gray-800:
            gray-800 = #3b3b3b
            # cat=bootstrap 4.x: styling/300/gray-900; type=string; label=$gray-900:
            gray-900 = #2b2b2b
            # cat=bootstrap 4.x: styling/300/black; type=string; label=$black:
            black = #000
            # cat=bootstrap 4.x: styling/300/blue; type=string; label=$blue:
            blue = #007bff
            # cat=bootstrap 4.x: styling/300/indigo; type=string; label=$indigo:
            indigo = #6610f2
            # cat=bootstrap 4.x: styling/300/purple; type=string; label=$purple:
            purple = #6f42c1
            # cat=bootstrap 4.x: styling/300/pink; type=string; label=$pink:
            pink = #e83e8c
            # cat=bootstrap 4.x: styling/300/red; type=string; label=$red:
            red = #dc3545
            # cat=bootstrap 4.x: styling/300/orange; type=string; label=$orange:
            orange = #fd7e14
            # cat=bootstrap 4.x: styling/300/yellow; type=string; label=$yellow:
            yellow = #ffc107
            # cat=bootstrap 4.x: styling/300/green; type=string; label=$green:
            green = #28a745
            # cat=bootstrap 4.x: styling/300/teal; type=string; label=$teal:
            teal = #20c997
            # cat=bootstrap 4.x: styling/300/cyan; type=string; label=$cyan:
            cyan = #17a2b8
            # cat=bootstrap 4.x: styling/300/default; type=string; label=$default:
            default = #eaebec
            # cat=bootstrap 4.x: styling/300/primary; type=string; label=$primary:
            primary = #4faf98
            # cat=bootstrap 4.x: styling/300/secondary; type=string; label=$secondary:
            secondary = #ff3f00
            # cat=bootstrap 4.x: styling/300/success; type=string; label=$success:
            success = $green
            # cat=bootstrap 4.x: styling/300/info; type=string; label=$info:
            info = $cyan
            # cat=bootstrap 4.x: styling/300/warning; type=string; label=$warning:
            warning = $yellow
            # cat=bootstrap 4.x: styling/300/danger; type=string; label=$danger:
            danger = $red
            # cat=bootstrap 4.x: styling/300/light; type=string; label=$light:
            light = $gray-100
            # cat=bootstrap 4.x: styling/300/dark; type=string; label=$dark:
            dark = $gray-900
            # cat=bootstrap 4.x: styling/300/theme-color-interval; type=string; label=$theme-color-interval:
            theme-color-interval = 8%
            # cat=bootstrap 4.x: styling/300/yiq-contrasted-threshold; type=string; label=$yiq-contrasted-threshold:
            yiq-contrasted-threshold = 150
            # cat=bootstrap 4.x: styling/300/yiq-text-dark; type=string; label=$yiq-text-dark:
            yiq-text-dark = $gray-900
            # cat=bootstrap 4.x: styling/300/yiq-text-light; type=string; label=$yiq-text-light:
            yiq-text-light = $white

            # cat=bootstrap 4.x: styling/301/enable-caret; type=options[Enabled=true, Disabled=false]; label=$enable-caret:
            enable-caret = true
            # cat=bootstrap 4.x: styling/301/enable-rounded; type=options[Enabled=true, Disabled=false]; label=$enable-rounded:
            enable-rounded = false
            # cat=bootstrap 4.x: styling/301/enable-shadows; type=options[Enabled=true, Disabled=false]; label=$enable-shadows:
            enable-shadows = false
            # cat=bootstrap 4.x: styling/301/enable-gradients; type=options[Enabled=true, Disabled=false]; label=$enable-gradients:
            enable-gradients = false
            # cat=bootstrap 4.x: styling/301/enable-transitions; type=options[Enabled=true, Disabled=false]; label=$enable-transitions:
            enable-transitions = true
            # cat=bootstrap 4.x: styling/301/enable-prefers-reduced-motion-media-query; type=options[Enabled=true, Disabled=false]; label=$enable-prefers-reduced-motion-media-query:
            enable-prefers-reduced-motion-media-query = true
            # cat=bootstrap 4.x: styling/301/enable-grid-classes; type=options[Enabled=true, Disabled=false]; label=$enable-grid-classes:
            enable-grid-classes = true
            # cat=bootstrap 4.x: styling/301/enable-pointer-cursor-for-buttons; type=options[Enabled=true, Disabled=false]; label=$enable-pointer-cursor-for-buttons:
            enable-pointer-cursor-for-buttons = true
            # cat=bootstrap 4.x: styling/301/enable-print-styles; type=options[Enabled=true, Disabled=false]; label=$enable-print-styles:
            enable-print-styles = true
            # cat=bootstrap 4.x: styling/301/enable-responsive-font-sizes; type=options[Enabled=true, Disabled=false]; label=$enable-responsive-font-sizes:
            enable-responsive-font-sizes = false
            # cat=bootstrap 4.x: styling/301/enable-validation-icons; type=options[Enabled=true, Disabled=false]; label=$enable-validation-icons:
            enable-validation-icons = true

            # cat=bootstrap 4.x: styling/302/spacer; type=string; label=$spacer:
            spacer = 1rem

            # cat=bootstrap 4.x: styling/303/body-bg; type=string; label=$body-bg:
            body-bg = $white
            # cat=bootstrap 4.x: styling/303/body-color; type=string; label=$body-color:
            body-color = $gray-900

            # cat=bootstrap 4.x: styling/304/link-color; type=string; label=$link-color:
            link-color = $primary
            # cat=bootstrap 4.x: styling/304/link-decoration; type=string; label=$link-decoration:
            link-decoration = none
            # cat=bootstrap 4.x: styling/304/link-hover-color; type=string; label=$link-hover-color:
            link-hover-color = darken($link-color, 15%)
            # cat=bootstrap 4.x: styling/304/link-hover-decoration; type=string; label=$link-hover-decoration:
            link-hover-decoration = underline
            # cat=bootstrap 4.x: styling/304/link-alternate-color; type=string; label=$link-alternate-color:
            link-alternate-color = #ffffff
            # cat=bootstrap 4.x: styling/304/link-alternate-hover-color; type=string; label=$link-alternate-hover-color:
            link-alternate-hover-color = darken($link-alternate-color, 15%)
            # cat=bootstrap 4.x: styling/304/emphasized-link-hover-darken-percentage; type=string; label=$emphasized-link-hover-darken-percentage:
            emphasized-link-hover-darken-percentage = 15%

            # cat=bootstrap 4.x: styling/305/paragraph-margin-bottom; type=string; label=$paragraph-margin-bottom:
            paragraph-margin-bottom = 1rem

            # cat=bootstrap 4.x: styling/306/grid-columns; type=string; label=$grid-columns:
            grid-columns = 12
            # cat=bootstrap 4.x: styling/306/grid-gutter-width; type=string; label=$grid-gutter-width:
            grid-gutter-width = 40px
            # cat=bootstrap 4.x: styling/306/grid-row-columns; type=string; label=$grid-gutter-width:
            grid-row-columns = 6

            # cat=bootstrap 4.x: styling/307/line-height-lg; type=string; label=$line-height-lg:
            line-height-lg = 1.5
            # cat=bootstrap 4.x: styling/307/line-height-sm; type=string; label=$line-height-sm:
            line-height-sm = 1.5
            # cat=bootstrap 4.x: styling/307/border-width; type=string; label=$border-width:
            border-width = 1px
            # cat=bootstrap 4.x: styling/307/border-color; type=string; label=$border-color:
            border-color = $gray-300
            # cat=bootstrap 4.x: styling/307/border-radius; type=string; label=$border-radius:
            border-radius = .25rem
            # cat=bootstrap 4.x: styling/307/border-radius-lg; type=string; label=$border-radius-lg:
            border-radius-lg = .3rem
            # cat=bootstrap 4.x: styling/307/border-radius-sm; type=string; label=$border-radius-sm:
            border-radius-sm = .2rem
            # cat=bootstrap 4.x: styling/307/rounded-pill; type=string; label=$rounded-pill:
            rounded-pill = 50rem;
            # cat=bootstrap 4.x: styling/307/component-active-color; type=string; label=$component-active-color:
            component-active-color = $white
            # cat=bootstrap 4.x: styling/307/component-active-bg; type=string; label=$component-active-bg:
            component-active-bg = $primary
            # cat=bootstrap 4.x: styling/307/caret-width; type=string; label=$caret-width:
            caret-width = .3em
            # cat=bootstrap 4.x: styling/307/caret-vertical-align; type=string; label=$caret-vertical-align:
            caret-vertical-align = $caret-width * .85
            # cat=bootstrap 4.x: styling/307/caret-spacing; type=string; label=$caret-spacing:
            caret-spacing = $caret-width * .85
            # cat=bootstrap 4.x: styling/307/transition-base; type=string; label=$transition-base:
            transition-base = all .2s ease-in-out
            # cat=bootstrap 4.x: styling/307/transition-fade; type=string; label=$transition-fade:
            transition-fade = opacity .15s linear
            # cat=bootstrap 4.x: styling/307/transition-collapse; type=string; label=$transition-collapse:
            transition-collapse = height .35s ease

            # cat=bootstrap 4.x: styling/308/font-family-sans-serif; type=string; label=$font-family-sans-serif:
            font-family-sans-serif = -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, "Noto Sans", sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol", "Noto Color Emoji"
            # cat=bootstrap 4.x: styling/308/font-family-monospace; type=string; label=$font-family-monospace:
            font-family-monospace = SFMono-Regular, Menlo, Monaco, Consolas, "Liberation Mono", "Courier New", monospace
            # cat=bootstrap 4.x: styling/308/font-family-base; type=string; label=$font-family-base:
            font-family-base = "#{$google-webfont}", sans-serif
            # cat=bootstrap 4.x: styling/308/font-size-base; type=string; label=$font-size-base:
            font-size-base = 1rem
            # cat=bootstrap 4.x: styling/308/font-size-lg; type=string; label=$font-size-lg:
            font-size-lg = $font-size-base * 1.25
            # cat=bootstrap 4.x: styling/308/font-size-sm; type=string; label=$font-size-sm:
            font-size-sm = $font-size-base * .875
            # cat=bootstrap 4.x: styling/308/font-weight-lighter; type=string; label=$font-weight-lighter:
            font-weight-lighter = lighter
            # cat=bootstrap 4.x: styling/308/font-weight-light; type=string; label=$font-weight-light:
            font-weight-light = 300
            # cat=bootstrap 4.x: styling/308/font-weight-normal; type=string; label=$font-weight-normal:
            font-weight-normal = 400
            # cat=bootstrap 4.x: styling/308/font-weight-bold; type=string; label=$font-weight-bold:
            font-weight-bold = 700
            # cat=bootstrap 4.x: styling/308/font-weight-bolder; type=string; label=$font-weight-bolder:
            font-weight-bolder = bolder
            # cat=bootstrap 4.x: styling/308/font-weight-base; type=string; label=$font-weight-base:
            font-weight-base = $font-weight-normal
            # cat=bootstrap 4.x: styling/308/line-height-base; type=string; label=$line-height-base:
            line-height-base = 1.5
            # cat=bootstrap 4.x: styling/308/h1-font-size; type=string; label=$h1-font-size:
            h1-font-size = $font-size-base * 2
            # cat=bootstrap 4.x: styling/308/h2-font-size; type=string; label=$h2-font-size:
            h2-font-size = $font-size-base * 1.75
            # cat=bootstrap 4.x: styling/308/h3-font-size; type=string; label=$h3-font-size:
            h3-font-size = $font-size-base * 1.5
            # cat=bootstrap 4.x: styling/308/h4-font-size; type=string; label=$h4-font-size:
            h4-font-size = $font-size-base * 1.25
            # cat=bootstrap 4.x: styling/308/h5-font-size; type=string; label=$h5-font-size:
            h5-font-size = $font-size-base * 1
            # cat=bootstrap 4.x: styling/308/h6-font-size; type=string; label=$h6-font-size:
            h6-font-size = $font-size-base * 0.85
            # cat=bootstrap 4.x: styling/308/headings-margin-bottom; type=string; label=$headings-margin-bottom:
            headings-margin-bottom = $spacer / 2
            # cat=bootstrap 4.x: styling/308/headings-font-family; type=string; label=$headings-font-family:
            headings-font-family = null
            # cat=bootstrap 4.x: styling/308/headings-font-weight; type=string; label=$headings-font-weight:
            headings-font-weight = 300
            # cat=bootstrap 4.x: styling/308/headings-line-height; type=string; label=$headings-line-height:
            headings-line-height = 1.2
            # cat=bootstrap 4.x: styling/308/headings-color; type=string; label=$headings-color:
            headings-color = null
            # cat=bootstrap 4.x: styling/308/display1-size; type=string; label=$display1-size:
            display1-size = 6rem
            # cat=bootstrap 4.x: styling/308/display2-size; type=string; label=$display2-size:
            display2-size = 5.5rem
            # cat=bootstrap 4.x: styling/308/display3-size; type=string; label=$display3-size:
            display3-size = 4.5rem
            # cat=bootstrap 4.x: styling/308/display4-size; type=string; label=$display4-size:
            display4-size = 3.5rem
            # cat=bootstrap 4.x: styling/308/display1-weight; type=string; label=$display1-weight:
            display1-weight = 300
            # cat=bootstrap 4.x: styling/308/display2-weight; type=string; label=$display2-weight:
            display2-weight = 300
            # cat=bootstrap 4.x: styling/308/display3-weight; type=string; label=$display3-weight:
            display3-weight = 300
            # cat=bootstrap 4.x: styling/308/display4-weight; type=string; label=$display4-weight:
            display4-weight = 300
            # cat=bootstrap 4.x: styling/308/display-line-height; type=string; label=$display-line-height:
            display-line-height = $headings-line-height
            # cat=bootstrap 4.x: styling/308/lead-font-size; type=string; label=$lead-font-size:
            lead-font-size = $font-size-base * 1.25
            # cat=bootstrap 4.x: styling/308/lead-font-weight; type=string; label=$lead-font-weight:
            lead-font-weight = 300
            # cat=bootstrap 4.x: styling/308/small-font-size; type=string; label=$small-font-size:
            small-font-size = 80%
            # cat=bootstrap 4.x: styling/308/text-muted; type=string; label=$text-muted:
            text-muted = $gray-600
            # cat=bootstrap 4.x: styling/308/blockquote-small-color; type=string; label=$blockquote-small-color:
            blockquote-small-color = $gray-600
            # cat=bootstrap 4.x: styling/308/blockquote-small-font-size; type=string; label=$blockquote-small-font-size:
            blockquote-small-font-size = $small-font-size
            # cat=bootstrap 4.x: styling/308/blockquote-font-size; type=string; label=$blockquote-font-size:
            blockquote-font-size = $font-size-base * 1.25
            # cat=bootstrap 4.x: styling/308/hr-border-color; type=string; label=$hr-border-color:
            hr-border-color = rgba($black, .1)
            # cat=bootstrap 4.x: styling/308/hr-border-width; type=string; label=$hr-border-width:
            hr-border-width = $border-width
            # cat=bootstrap 4.x: styling/308/mark-padding; type=string; label=$mark-padding:
            mark-padding = .2em
            # cat=bootstrap 4.x: styling/308/dt-font-weight; type=string; label=$dt-font-weight:
            dt-font-weight = $font-weight-bold
            # cat=bootstrap 4.x: styling/308/kbd-box-shadow; type=string; label=$kbd-box-shadow:
            kbd-box-shadow = inset 0 -.1rem 0 rgba($black, .25)
            # cat=bootstrap 4.x: styling/308/nested-kbd-font-weight; type=string; label=$nested-kbd-font-weight:
            nested-kbd-font-weight = $font-weight-bold
            # cat=bootstrap 4.x: styling/308/list-inline-padding; type=string; label=$list-inline-padding:
            list-inline-padding = .5rem
            # cat=bootstrap 4.x: styling/308/mark-bg; type=string; label=$mark-bg:
            mark-bg = #fcf8e3
            # cat=bootstrap 4.x: styling/308/hr-margin-y; type=string; label=$hr-margin-y:
            hr-margin-y = $spacer

            # cat=bootstrap 4.x: styling/309/table-cell-padding; type=string; label=$table-cell-padding:
            table-cell-padding = .75rem
            # cat=bootstrap 4.x: styling/309/table-cell-padding-sm; type=string; label=$table-cell-padding-sm:
            table-cell-padding-sm = .3rem
            # cat=bootstrap 4.x: styling/309/table-color; type=string; label=$table-color:
            table-color = $body-color
            # cat=bootstrap 4.x: styling/309/table-bg; type=string; label=$table-bg:
            table-bg = null
            # cat=bootstrap 4.x: styling/309/table-accent-bg; type=string; label=$table-accent-bg:
            table-accent-bg = rgba($black, .05)
            # cat=bootstrap 4.x: styling/309/table-hover-color; type=string; label=$table-hover-color:
            table-hover-color = $table-color
            # cat=bootstrap 4.x: styling/309/table-hover-bg; type=string; label=$table-hover-bg:
            table-hover-bg = rgba($black, .075)
            # cat=bootstrap 4.x: styling/309/table-active-bg; type=string; label=$table-active-bg:
            table-active-bg = $table-hover-bg
            # cat=bootstrap 4.x: styling/309/table-border-width; type=string; label=$table-border-width:
            table-border-width = $border-width
            # cat=bootstrap 4.x: styling/309/table-border-color; type=string; label=$table-border-color:
            table-border-color = $border-color
            # cat=bootstrap 4.x: styling/309/table-head-bg; type=string; label=$table-head-bg:
            table-head-bg = $gray-200
            # cat=bootstrap 4.x: styling/309/table-head-color; type=string; label=$table-head-color:
            table-head-color = $gray-700
            # cat=bootstrap 4.x: styling/309/table-dark-color; type=string; label=$table-dark-color:
            table-dark-color = $white
            # cat=bootstrap 4.x: styling/309/table-dark-bg; type=string; label=$table-dark-bg:
            table-dark-bg = $gray-900
            # cat=bootstrap 4.x: styling/309/table-dark-accent-bg; type=string; label=$table-dark-accent-bg:
            table-dark-accent-bg = rgba($white, .05)
            # cat=bootstrap 4.x: styling/309/table-dark-hover-color; type=string; label=$table-dark-hover-color:
            table-dark-hover-color = $table-dark-color
            # cat=bootstrap 4.x: styling/309/table-dark-hover-bg; type=string; label=$table-dark-hover-bg:
            table-dark-hover-bg = rgba($white, .075)
            # cat=bootstrap 4.x: styling/309/table-dark-border-color; type=string; label=$table-dark-border-color:
            table-dark-border-color = lighten($table-dark-bg, 7.5%)
            # cat=bootstrap 4.x: styling/309/table-striped-order; type=string; label=$table-striped-order:
            table-striped-order = odd
            # cat=bootstrap 4.x: styling/309/table-caption-color; type=string; label=$table-caption-color:
            table-caption-color = $text-muted
            # cat=bootstrap 4.x: styling/309/table-bg-level; type=string; label=$table-bg-level:
            table-bg-level = -9
            # cat=bootstrap 4.x: styling/309/table-border-level; type=string; label=$table-border-level:
            table-border-level = -6

            # cat=bootstrap 4.x: styling/310/input-btn-padding-y; type=string; label=$input-btn-padding-y:
            input-btn-padding-y = .375rem
            # cat=bootstrap 4.x: styling/310/input-btn-padding-x; type=string; label=$input-btn-padding-x:
            input-btn-padding-x = .75rem
            # cat=bootstrap 4.x: styling/310/input-btn-font-family; type=string; label=$input-btn-font-family:
            input-btn-font-family = null
            # cat=bootstrap 4.x: styling/310/input-btn-font-size; type=string; label=$input-btn-font-size:
            input-btn-font-size = $font-size-base
            # cat=bootstrap 4.x: styling/310/input-btn-line-height; type=string; label=$input-btn-line-height:
            input-btn-line-height = $line-height-base
            # cat=bootstrap 4.x: styling/310/input-btn-focus-width; type=string; label=$input-btn-focus-width:
            input-btn-focus-width = .2rem
            # cat=bootstrap 4.x: styling/310/input-btn-focus-color; type=string; label=$input-btn-focus-color:
            input-btn-focus-color = rgba($component-active-bg, .25)
            # cat=bootstrap 4.x: styling/310/input-btn-focus-box-shadow; type=string; label=$input-btn-focus-box-shadow:
            input-btn-focus-box-shadow = 0 0 0 $input-btn-focus-width $input-btn-focus-color
            # cat=bootstrap 4.x: styling/310/input-btn-padding-y-sm; type=string; label=$input-btn-padding-y-sm:
            input-btn-padding-y-sm = .25rem
            # cat=bootstrap 4.x: styling/310/input-btn-padding-x-sm; type=string; label=$input-btn-padding-x-sm:
            input-btn-padding-x-sm = .5rem
            # cat=bootstrap 4.x: styling/310/input-btn-font-size-sm; type=string; label=$input-btn-font-size-sm:
            input-btn-font-size-sm = $font-size-sm
            # cat=bootstrap 4.x: styling/310/input-btn-line-height-sm; type=string; label=$input-btn-line-height-sm:
            input-btn-line-height-sm = $line-height-sm
            # cat=bootstrap 4.x: styling/310/input-btn-padding-y-lg; type=string; label=$input-btn-padding-y-lg:
            input-btn-padding-y-lg = .5rem
            # cat=bootstrap 4.x: styling/310/input-btn-padding-x-lg; type=string; label=$input-btn-padding-x-lg:
            input-btn-padding-x-lg = 1rem
            # cat=bootstrap 4.x: styling/310/input-btn-font-size-lg; type=string; label=$input-btn-font-size-lg:
            input-btn-font-size-lg = $font-size-lg
            # cat=bootstrap 4.x: styling/310/input-btn-line-height-lg; type=string; label=$input-btn-line-height-lg:
            input-btn-line-height-lg = $line-height-lg
            # cat=bootstrap 4.x: styling/310/input-btn-border-width; type=string; label=$input-btn-border-width:
            input-btn-border-width = $border-width

            # cat=bootstrap 4.x: styling/311/btn-padding-y; type=string; label=$btn-padding-y:
