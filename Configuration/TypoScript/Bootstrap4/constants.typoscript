#####################
### BOOTSTRAP 4.x ###
#####################
plugin.bootstrap_package.settings.scss >

############################
### CUSTOM SUBCATEGORIES ###
############################
# customsubcategory=bp4_001=Grayscale
# customsubcategory=bp4_002=Colors
# customsubcategory=bp4_003=Contrast
# customsubcategory=bp4_004=Body
# customsubcategory=bp4_005=Links
# customsubcategory=bp4_006=Fonts
# customsubcategory=bp4_010=Options

plugin.bootstrap_package {
    settings {
        scss {

            # cat=bootstrap 4.x: styling/bp4_001/001; type=string; label=$white:
            white = #ffffff
            # cat=bootstrap 4.x: styling/bp4_001/002; type=string; label=$gray-100:
            gray-100 = #f8f8f8
            # cat=bootstrap 4.x: styling/bp4_001/003; type=string; label=$gray-200:
            gray-200 = #e9e9e9
            # cat=bootstrap 4.x: styling/bp4_001/004; type=string; label=$gray-300:
            gray-300 = #dedede
            # cat=bootstrap 4.x: styling/bp4_001/005; type=string; label=$gray-400:
            gray-400 = #cecece
            # cat=bootstrap 4.x: styling/bp4_001/006; type=string; label=$gray-500:
            gray-500 = #ababab
            # cat=bootstrap 4.x: styling/bp4_001/007; type=string; label=$gray-600:
            gray-600 = #6c6c6c
            # cat=bootstrap 4.x: styling/bp4_001/008; type=string; label=$gray-700:
            gray-700 = #494949
            # cat=bootstrap 4.x: styling/bp4_001/009; type=string; label=$gray-800:
            gray-800 = #313131
            # cat=bootstrap 4.x: styling/bp4_001/010; type=string; label=$gray-900:
            gray-900 = #212121
            # cat=bootstrap 4.x: styling/bp4_001/011; type=string; label=$black:
            black = #000000

            # cat=bootstrap 4.x: styling/bp4_002/001; type=string; label=$primary:
            primary = #2a9d8f
            # cat=bootstrap 4.x: styling/bp4_002/002; type=string; label=$secondary:
            secondary = #e76f51
            # cat=bootstrap 4.x: styling/bp4_002/003; type=string; label=$tertiary:
            tertiary = #f4a261
            # cat=bootstrap 4.x: styling/bp4_002/004; type=string; label=$quaternary:
            quaternary = #e9c46a

            # cat=bootstrap 4.x: styling/bp4_002/005; type=string; label=$default:
            default = #eaebec
            # cat=bootstrap 4.x: styling/bp4_002/006; type=string; label=$success:
            success = #5cb85c
            # cat=bootstrap 4.x: styling/bp4_002/007; type=string; label=$info:
            info = #319fc0
            # cat=bootstrap 4.x: styling/bp4_002/008; type=string; label=$warning:
            warning = #f0ad4e
            # cat=bootstrap 4.x: styling/bp4_002/009; type=string; label=$danger:
            danger = #d9534f
            # cat=bootstrap 4.x: styling/bp4_002/010; type=string; label=$lighter:
            lighter = $gray-100
            # cat=bootstrap 4.x: styling/bp4_002/011; type=string; label=$light:
            light = $gray-200
            # cat=bootstrap 4.x: styling/bp4_002/012; type=string; label=$dark:
            dark = $gray-800
            # cat=bootstrap 4.x: styling/bp4_002/013; type=string; label=$darker:
            darker = $gray-900

            # cat=bootstrap 4.x: styling/bp4_003/001; type=string; label=$yiq-contrasted-threshold:
            yiq-contrasted-threshold = 160

            # cat=bootstrap 4.x: styling/bp4_004/001; type=string; label=$body-bg:
            body-bg = $white
            # cat=bootstrap 4.x: styling/bp4_004/002; type=string; label=$body-color:
            body-color = $gray-900

            # cat=bootstrap 4.x: styling/bp4_005/001; type=string; label=$link-color:
            link-color = theme-color("primary")
            # cat=bootstrap 4.x: styling/bp4_005/002; type=string; label=$link-decoration:
            link-decoration = none
            # cat=bootstrap 4.x: styling/bp4_005/003; type=string; label=$link-hover-color:
            link-hover-color = darken($link-color, 15%)
            # cat=bootstrap 4.x: styling/bp4_005/004; type=string; label=$link-hover-decoration:
            link-hover-decoration = underline

            # cat=bootstrap 4.x: styling/bp4_006/001; type=string; label=$font-family-sans-serif:
            font-family-sans-serif = "#{$google-webfont}", sans-serif
            # cat=bootstrap 4.x: styling/bp4_006/002; type=string; label=$font-family-monospace:
            font-family-monospace = SFMono-Regular, Menlo, Monaco, Consolas, "Liberation Mono", "Courier New", monospace

            # cat=bootstrap 4.x: styling/bp4_010/001; type=options[Enabled=true, Disabled=false]; label=$enable-rounded:
            enable-rounded = true
            # cat=bootstrap 4.x: styling/bp4_010/002; type=options[Enabled=true, Disabled=false]; label=$enable-shadows:
            enable-shadows = true
            # cat=bootstrap 4.x: styling/bp4_010/003; type=options[Enabled=true, Disabled=false]; label=$enable-gradients:
            enable-gradients = false
            # cat=bootstrap 4.x: styling/bp4_010/004; type=options[Enabled=true, Disabled=false]; label=$enable-transitions:
            enable-transitions = true

        }
    }
}
