            # cat=bootstrap 4.x: styling/333/close-font-size; type=string; label=$close-font-size:
            close-font-size = $font-size-base * 1.5
            # cat=bootstrap 4.x: styling/333/close-font-weight; type=string; label=$close-font-weight:
            close-font-weight = $font-weight-bold
            # cat=bootstrap 4.x: styling/333/close-color; type=string; label=$close-color:
            close-color = $black
            # cat=bootstrap 4.x: styling/333/close-text-shadow; type=string; label=$close-text-shadow:
            close-text-shadow = 0 1px 0 $white

            # cat=bootstrap 4.x: styling/334/code-font-size; type=string; label=$code-font-size:
            code-font-size = 87.5%
            # cat=bootstrap 4.x: styling/334/code-color; type=string; label=$code-color:
            code-color = $pink
            # cat=bootstrap 4.x: styling/334/kbd-padding-y; type=string; label=$kbd-padding-y:
            kbd-padding-y = .2rem
            # cat=bootstrap 4.x: styling/334/kbd-padding-x; type=string; label=$kbd-padding-x:
            kbd-padding-x = .4rem
            # cat=bootstrap 4.x: styling/334/kbd-font-size; type=string; label=$kbd-font-size:
            kbd-font-size = $code-font-size
            # cat=bootstrap 4.x: styling/334/kbd-color; type=string; label=$kbd-color:
            kbd-color = $white
            # cat=bootstrap 4.x: styling/334/kbd-bg; type=string; label=$kbd-bg:
            kbd-bg = $gray-900
            # cat=bootstrap 4.x: styling/334/pre-color; type=string; label=$pre-color:
            pre-color = $gray-900
            # cat=bootstrap 4.x: styling/334/pre-scrollable-max-height; type=string; label=$pre-scrollable-max-height:
            pre-scrollable-max-height = 340px

            # cat=bootstrap 4.x: styling/335/print-page-size; type=string; label=$print-page-size:
            print-page-size = a3
            # cat=bootstrap 4.x: styling/335/print-body-min-width; type=string; label=$print-body-min-width:
            print-body-min-width = map-get($grid-breakpoints, "lg")

        }
    }
}
